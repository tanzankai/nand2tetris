/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=w1);
    Nand(a=a, b=b, out=w2);
    Nand(a=w1, b=w2, out=w3);
    Not(in=w3, out=out);
}